{
  "compilerOptions": {
    "baseUrl": ".",
    "target": "es5",
    "lib": ["dom", "dom.iterable", "esnext", "es5"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "commonjs",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "paths": {
      "@/*": ["./*"]
    },
    "types": ["cypress", "node"],
    "plugins": [
      {
        "name": "next"
      }
    ]
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    "**/*.jsx",
    "pages/api/all-apis",
    ".next/types/**/*.ts",
    "cypress.config.mjs",
    "cypress.config.mjs",
    "cypress/**/*.ts",
    "cypress",
    "environment.d.ts",
    "cypress.d.ts"
  ],
  "exclude": [
    "node_modules",
    "**/*.cy.ts",
    "**/*.cy.tsx",
    "./cypress/support/e2e.ts"
  ],
  "files": ["environment.d.ts"]
}
