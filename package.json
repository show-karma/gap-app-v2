  "private": true,
  "packageManager": "bun@latest",
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build --turbopack",
    "start": "next start",
    "lint": "biome check .",
    "lint:fix": "biome check --write .",
    "format": "biome format --write .",
    "postbuild": "next-sitemap",
    "build-stats": "ANALYZE=true bun run build",
    "test": "bun test",
    "test:watch": "bun test --watch",
    "test:coverage": "bun test --coverage",
    "test:unit": "bun test __tests__/unit",
    "test:integration": "bun test __tests__/integration",
    "test:pages": "bun test __tests__/integration/pages",
    "test:fast": "bun test __tests__/unit --timeout 5000",
    "test:debug": "bun test --bail",
    "test:jest": "cross-env NODE_OPTIONS=--max-old-space-size=4096 jest --maxWorkers=50%",
    "test:jest:watch": "cross-env NODE_OPTIONS=--max-old-space-size=4096 jest --watch",
    "test:metrics:collect": "bun scripts/test-metrics.js collect",
    "test:metrics:show": "bun scripts/test-metrics.js show",
    "test:metrics:report": "bun scripts/test-metrics.js report",
    "cypress:open": "cypress open",
    "e2e": "start-server-and-test dev http://localhost:3000 \"bun cypress open --e2e\"",
    "e2e:headless": "start-server-and-test dev http://localhost:3000 \"bun cypress run --e2e\"",
    "e2e:ci": "start-server-and-test start http://localhost:3000 \"bun cypress run --e2e\"",
    "component": "cypress open --component",
    "component:headless": "cypress run --component",
    "prepare": "husky",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "chromatic": "chromatic --exit-zero-on-changes",
    "lighthouse": "lhci autorun",
