    "start": "next start",
    "lint": "next lint",
    "postbuild": "next-sitemap",
    "build-stats": "cross-env ANALYZE=true yarn build",
    "test": "cross-env NODE_OPTIONS=--max-old-space-size=4096 jest --maxWorkers=50%",
    "test:watch": "cross-env NODE_OPTIONS=--max-old-space-size=4096 jest --watch",
    "test:coverage": "cross-env NODE_OPTIONS=--max-old-space-size=4096 jest --coverage --maxWorkers=50%",
    "test:unit": "cross-env NODE_OPTIONS=--max-old-space-size=4096 jest __tests__/unit",
    "test:integration": "cross-env NODE_OPTIONS=--max-old-space-size=4096 jest __tests__/integration",
    "test:pages": "cross-env NODE_OPTIONS=--max-old-space-size=4096 jest __tests__/integration/pages",
    "test:fast": "cross-env NODE_OPTIONS=--max-old-space-size=2048 jest __tests__/unit --maxWorkers=1 --testTimeout=5000",
    "test:debug": "cross-env NODE_OPTIONS=--max-old-space-size=4096 jest --runInBand --detectOpenHandles --logHeapUsage",
